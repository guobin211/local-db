{
  "name": "local-db",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:app": "pnpm tauri dev --no-watch",
    "build": "tsc && vite build",
    "build:app": "node build.js",
    "preview": "vite preview",
